<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Esprit\DemoBundle\Entity\Client" table="client">
    <id name="id" type="integer" column="IdClient">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="adressmail" type="string" column="AdressMail" length="50" nullable="false"/>
    <field name="banir" type="integer" column="Banir" nullable="false"/>
    <field name="login" type="string" column="Login" length="30" nullable="false"/>
    <field name="motdepasse" type="string" column="MotDePasse" length="30" nullable="false"/>
    <field name="nom" type="string" column="Nom" length="30" nullable="false"/>
    <field name="numtel" type="integer" column="NumTel" nullable="false"/>
    <field name="prenom" type="string" column="Prenom" length="30" nullable="false"/>
  </entity>
</doctrine-mapping>
