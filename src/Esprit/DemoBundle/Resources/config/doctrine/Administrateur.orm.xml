<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Esprit\DemoBundle\Entity\Administrateur" table="administrateur">
    <id name="id" type="integer" column="IdAdministrateur">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="datederniervisite" type="string" column="DateDernierVisite" nullable="false"/>
    <field name="login" type="string" column="login" length="30" nullable="false"/>
    <field name="motdepasse" type="string" column="MotDePasse" length="30" nullable="false"/>
  </entity>
</doctrine-mapping>
